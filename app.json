{
  "expo": {
    "name": "Andrew Mainella",
    "slug": "Andrew_Mainella",
    "scheme": "com.Archimedes4.AndrewMainella",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c93ba"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Archimedes4.AndrewMainella",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1c93ba"
      },
      "package": "com.Archimedes4.Andrew_Mainella"
    },
    "web": {
      "favicon": "./assets/favicon.jpg",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-dev-launcher",
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "e37f58d3-106c-4403-9f4d-f65867548aec"
      }
    }
  }
}
